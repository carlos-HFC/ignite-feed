{
  "extends": [
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "array-callback-return": "off",
    "react/jsx-key": "off",
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "singleAttributePerLine": true
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "ignorePatterns": [
    "node_modules"
  ]
}